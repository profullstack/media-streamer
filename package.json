{
  "name": "media-streamer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "git config core.hooksPath .githooks || true",
    "precommit": "pnpm build && pnpm test && pnpm lint",
    "supabase:update-emails": "tsx --env-file=.env scripts/supabase-update-emails.ts",
    "supabase:delete-torrent": "tsx --env-file=.env scripts/supabase-delete-infohash.ts",
    "enrich-metadata": "tsx --env-file=.env scripts/enrich-torrent-metadata.ts",
    "enrich-metadata:all": "tsx --env-file=.env scripts/enrich-torrent-metadata.ts --all-status",
    "enrich-metadata:dry-run": "tsx --env-file=.env scripts/enrich-torrent-metadata.ts --all-status --dry-run",
    "enrich-metadata:force": "tsx --env-file=.env scripts/enrich-torrent-metadata.ts --all-status --force",
    "enrich-folders": "tsx --env-file=.env scripts/enrich-folder-metadata.ts",
    "enrich-folders:all": "tsx --env-file=.env scripts/enrich-folder-metadata.ts --all-status",
    "enrich-folders:dry-run": "tsx --env-file=.env scripts/enrich-folder-metadata.ts --all-status --dry-run",
    "enrich-folders:force": "tsx --env-file=.env scripts/enrich-folder-metadata.ts --all-status --force",
    "iptv-worker": "tsx --env-file=.env workers/iptv-cache/index.ts",
    "iptv-worker:dev": "tsx --watch --env-file=.env workers/iptv-cache/index.ts",
    "podcast-worker": "tsx --env-file=.env workers/podcast-notifier/index.ts",
    "podcast-worker:dev": "tsx --watch --env-file=.env workers/podcast-notifier/index.ts"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@supabase/supabase-js": "^2.89.0",
    "@types/video.js": "^7.3.58",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "epubjs": "^0.3.93",
    "foliate-js": "^1.0.1",
    "framer-motion": "^12.23.26",
    "hls.js": "^1.6.15",
    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.35.0",
    "linkedom": "^0.18.12",
    "lucide-react": "^0.468.0",
    "mpegts.js": "^1.8.0",
    "next": "16.1.1",
    "node-datachannel": "^0.31.0",
    "openai": "^6.15.0",
    "puppeteer": "^24.34.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-pdf": "^10.3.0",
    "resend": "^6.6.0",
    "sax": "^1.4.3",
    "tailwind-merge": "^2.6.0",
    "undici": "^7.16.0",
    "video.js": "^8.23.4",
    "web-push": "^3.6.7",
    "webtorrent": "^2.8.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "27.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/sax": "^1.2.7",
    "@types/web-push": "^3.6.4",
    "@types/webtorrent": "^0.109.10",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "16.1.1",
    "jsdom": "27.4.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@9.15.1"
}
